pipeline {
    environment {
        imagename = "paivae/mundose-web"
        dockerImage = ''
        containerName = 'mundose-webapp-test'
        dockerHubCredentials = 'DockerHubs'
    }
    agent any
    stages {


        stage('Deploy Image') {
            steps {
                sshagent(credentials: ['webapp-prod-serv']) {
                    sh 'ssh -o StrictHostKeyChecking=no -l root web.mundose.paivae.com uname -a'
                }
            }
        }
    }   

    post {
    always {
      sh 'docker logout'
      echo 'Session cerrada de docker'
    }
  }
}

